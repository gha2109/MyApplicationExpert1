<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.InterventionDetail.interventionDetailFragment">


    <androidx.gridlayout.widget.GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="187dp"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="0">

            <androidx.gridlayout.widget.GridLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="235dp"
                    android:layout_height="42dp"
                    android:background="#139AC5"
                    android:backgroundTint="#389ED6"
                    app:layout_column="0"
                    app:layout_row="0" />

                <TableRow
                    android:layout_width="247dp"
                    android:layout_height="42dp"
                    android:background="#389ED6"
                    android:backgroundTint="#389ED6"
                    app:layout_column="1"
                    app:layout_row="0" />

                <TableRow
                    android:layout_width="235dp"
                    android:layout_height="145dp"
                    app:layout_column="0"
                    app:layout_row="1" />

                <TableRow
                    android:layout_width="249dp"
                    android:layout_height="143dp"
                    app:layout_column="1"
                    app:layout_row="1" />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="235dp"
                    android:layout_height="46dp"
                    android:text="Planifié"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    app:layout_column="0"
                    app:layout_row="0" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="228dp"
                    android:layout_height="32dp"
                    android:text="Effectué"
                    android:textAlignment="center"
                    android:textColor="#F3EFEF"
                    android:textSize="16sp"
                    app:layout_column="1"
                    app:layout_row="0" />

                <androidx.gridlayout.widget.GridLayout
                    android:layout_width="232dp"
                    android:layout_height="142dp"
                    app:layout_column="0"
                    app:layout_row="1">

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:text="Heure: 7:00H 10:00H"
                        android:textSize="14sp"
                        app:layout_column="0"
                        app:layout_row="0" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="Temps Travaillé : 00:00"
                        app:layout_column="0"
                        app:layout_row="1" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Action"
                        app:layout_column="0"
                        app:layout_row="2" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="Commentaire : vide"
                        app:layout_column="0"
                        app:layout_row="4" />

                    <TextView
                        android:id="@+id/textView11"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="Evenement N1 (00:00)"
                        app:layout_column="0"
                        app:layout_row="3" />
                </androidx.gridlayout.widget.GridLayout>

                <androidx.gridlayout.widget.GridLayout
                    android:layout_width="247dp"
                    android:layout_height="144dp"
                    app:layout_column="1"
                    app:layout_row="1">

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="Terminé: oui"
                        app:layout_column="0"
                        app:layout_row="0" />

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="21/08/2018 - 22/08/2018"
                        app:layout_column="0"
                        app:layout_row="1" />

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Heure : 7:00H  - 10:00H  "
                        app:layout_column="0"
                        app:layout_row="2" />

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="match_parent"
                        android:layout_height="29dp"
                        android:text="Temps Travaillé: (00:00)"
                        app:layout_column="0"
                        app:layout_row="3" />

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Action"
                        app:layout_column="0"
                        app:layout_row="4" />

                    <TextView
                        android:id="@+id/textView28"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="commentaire:rien a regarder"
                        app:layout_column="0"
                        app:layout_row="5" />
                </androidx.gridlayout.widget.GridLayout>

            </androidx.gridlayout.widget.GridLayout>
        </LinearLayout>

        <Button
            android:id="@+id/button13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#2196F3"
            android:text="Client N1"
            app:layout_column="0"
            app:layout_row="1" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="219dp"
            app:layout_column="0"
            app:layout_row="2">

            <androidx.gridlayout.widget.GridLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView30"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:text="Nom complet : Jean Paul"
                    app:layout_column="0"
                    app:layout_row="0" />

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:text="titre : La societe exemple"
                    app:layout_column="0"
                    app:layout_row="1" />

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="match_parent"
                    android:layout_height="31dp"
                    android:text="Société: La Société Exemple"
                    app:layout_column="0"
                    app:layout_row="2" />

                <TextView
                    android:id="@+id/textView33"
                    android:layout_width="match_parent"
                    android:layout_height="29dp"
                    android:text="Email: Jean@gmail.com"
                    app:layout_column="0"
                    app:layout_row="3" />

                <TextView
                    android:id="@+id/textView34"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:text="Contact:0612346587"
                    app:layout_column="0"
                    app:layout_row="4" />

                <Button
                    android:id="@+id/button21"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#03A9F4"
                    android:text="Envoyer un Rapport au client"
                    app:layout_column="0"
                    app:layout_row="5" />
            </androidx.gridlayout.widget.GridLayout>
        </TableRow>

        <Button
            android:id="@+id/button22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#2196F3"
            android:text="Adresse"
            app:layout_column="0"
            app:layout_row="3"
            tools:ignore="TouchTargetSizeCheck" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="81dp"
            app:layout_column="0"
            app:layout_row="4" >

        </TableRow>

        <Button
            android:id="@+id/button23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#2196F3"
            android:text="Piece jointe"
            app:layout_column="0"
            app:layout_row="5" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="62dp"
            app:layout_column="0"
            app:layout_row="9" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:text="TextView"
            app:layout_column="0"
            app:layout_row="6" />

        <Button
            android:id="@+id/button24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#2196F3"
            android:text="Historique"
            app:layout_column="0"
            app:layout_row="7" />

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="match_parent"
            android:layout_height="111dp"
            app:layout_column="0"
            app:layout_row="4">

            <Button
                android:id="@+id/carte"
                android:layout_width="126dp"
                android:layout_height="wrap_content"
                android:backgroundTint="#03A9F4"
                android:gravity="center"
                android:text="Carte"
                android:verticalScrollbarPosition="defaultPosition"
                app:layout_column="0"
                app:layout_row="1" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:text="Adresse : Rue Paradis"
                app:layout_column="0"
                app:layout_row="0" />
        </androidx.gridlayout.widget.GridLayout>

        <Button
            android:id="@+id/Historique"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#03A9F4"
            android:text="Historique "
            app:layout_column="0"
            app:layout_row="9" />

    </androidx.gridlayout.widget.GridLayout>

</FrameLayout>